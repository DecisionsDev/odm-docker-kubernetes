{
  "httpHeaders": [
    {
      "urls": ["https://www.ubisecure.com/access-management/private_key_jwt-or-client_secret/"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "ignorePatterns": [
    {
      "pattern": "^http://localhost",
      "pattern": "^https://jwt.io"
    }
  ],
  "replacementPatterns": [
      {
        "pattern": "^/LICENSE",
        "replacement": "{{BASEURL}}/LICENSE"
      },
      {
        "pattern": "https://www.ibm.com/docs",
        "replacement": "https://ibmdocs-test.dcs.ibm.com/docs"
      }
    ],

    "timeout": "20s",
    "retryOn429": true,
    "retryCount": 5,
    "fallbackRetryDelay": "30s"
  }
